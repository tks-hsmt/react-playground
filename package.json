{
  "name": "react-playground",
  "homepage": "/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format:check": "prettier --check --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,cjs,json,css}'",
    "format:fix": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,cjs,json,css}'",
    "lint:eslint": "eslint --ignore-path .gitignore .  --ext .js,.jsx,.ts,.tsx,cjs",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:style": "stylelint --ignore-path .gitignore \"src/**/*.+(css|ts|tsx)\"",
    "lint:style:fix": "npm run lint:style -- --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@tanstack/react-query": "^4.29.12",
    "axios": "^1.4.0",
    "fs-extra": "^11.1.1",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.43.9",
    "react-router-dom": "^6.11.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@mswjs/data": "^0.12.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^20.1.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "^5.10.3",
    "msw": "^1.2.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.3",
    "typescript": "*",
    "vite": "^4.3.2"
  },
  "engines": {
    "node": "18.16.0",
    "npm": "9.5.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
